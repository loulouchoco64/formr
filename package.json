{
    "name": "@klicat/riot-formr",
    "version": "1.1.0",
    "private": false,
    "description": "Riot 4 form generator",
    "scripts": {
        "start": "(start npm run watch && start npm run server) || (npm run watch & npm run server)",
        "build": "rollup -c",
        "watch": "rollup -c --watch",
        "server": "browser-sync dist -w --index demo.html",
