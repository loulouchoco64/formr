    },
    "dependencies": {
        "npm": "^6.11.3",
        "riot": "^4.6.2"
    },
    "devDependencies": {
        "@riotjs/compiler": "^4.4.3",
        "browser-sync": "^2.26.7",
        "rollup": "^1.21.4",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-copy": "^3.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-riot": "^4.0.0"
    },
    "license": "MIT",
    "author": "klicat",
    "main": "dist/riot-formr.js",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "riot",
        "form"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/infuzz/riot-formr.git"
    },
    "bugs": {
        "url": "https://github.com/infuzz/riot-formr/issues"
